{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "^2.4",
        "cmgmyr/messenger": "^2.14",
        "codecourse/notify": "^1.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "league/fractal": "^0.16.0",
        "maatwebsite/excel": "~2.1.0",
        "silber/bouncer": "v1.0.0-beta.3",
        "spatie/laravel-fractal": "^4.01",
        "spatie/laravel-html": "^2",
        "spatie/laravel-migrate-fresh": "^1.5",
        "tymon/jwt-auth": "0.5.*",
        "yajra/laravel-datatables-buttons": "^2.0",
        "yajra/laravel-datatables-oracle": "^7.5"
    },
    "require-dev": {
        "doctrine/dbal": "~2.6",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
          "app/Http/Helpers.php",
          "tests/utilities/functions.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
